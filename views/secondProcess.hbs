    <div id="main_div"> 
  <form id="sp" action="/user/addProcessTwo" method="POST">
    <div class="form-row">
      <div class="form-group col-md-6">
        <label for="fname">21. Process ID</label>
      
      
        <input type="text-center"  class="form-control" name="Proc_Id" value="{{list.Proc_Id}}" readonly>
      </div>
    


    
      {{!-- <div class="col-20">
        <label for="fname">clientName</label>
      </div> --}}
      
        <input type="hidden"  class="form-control" name="clientName" value="{{list.clientName}}" readonly>
      
    


    
      {{!-- <div class="col-20">
        <label for="fname">BusinessUnit</label>
      </div> --}}
      
        <input type="hidden"  class="form-control" name="Buss_Unit" value="{{list.Buss_Unit}}" readonly>
      
    


    
      {{!-- <div class="col-20">
        <label for="fname">SBU</label>
      </div> --}}
    
        <input type="hidden"  class="form-control" name="Sub_Buss_Unit" value="{{list.Sub_Buss_Unit}}" readonly>
      
    

    
      {{!-- <div class="col-20">
        <label for="fname">ProcessName</label>
      </div> --}}
      
        <input type="hidden"  class="form-control" name="Proc_Name" value="{{list.Proc_Name}}" readonly>
      
    
            <input type="hidden"  class="form-control" id="mvtwo" maxlength="30" value="{{list.Mon_Vol}}" name="Mon_Vol">
        
        {{!-- <div class="col-20">
          <label for="country">8. AHT(in minutes)</label>  
        </div> --}}
        
            <input type="hidden"  class="form-control" id="ahttwo" step="any" maxlength="30" value="{{list.AHT}}" name="AHT">
        
     
              {{!-- <div class="col-20">
                <label for="sla">20. Percentage of Automation Potential</label>
              </div> --}}
              
                      <input class="form-control"  min="0" max="100" id="apottwo" type="hidden" value="{{list.AP_Perc}}" step="any" name="AP_Perc">
              
    <div class="form-group col-md-6">
        <label for="fname">22. Number of applications involved in the process</label>
      
      
        <input type="text-center" oninput="funtional_point()" id="noa" class="form-control"  name="Num_of_apps" onkeypress="return isNumberKey(event)"  required>
      </div>
    </div>
    <div class="form-row">
      <div class="form-group col-md-6">
        <label for="lname">23. Number of mainframe applications involved</label>
      
      
        <input type="text-center" oninput="funtional_point()" id="nom" class="form-control" name="Num_of_mainframe" onkeypress="return isNumberKey(event)"  required>
      </div>
    <div class="form-group col-md-6">
        <label for="country">24. Number of applications to be accessed through Citrix</label>  
      
      
        <input type="text-center" oninput="funtional_point()" id= "noc" class="form-control"  name="Num_of_Citrix" onkeypress="return isNumberKey(event)"  required>
      </div>
    </div>
    <div class="form-row">
        <div class="form-group col-md-6">
          <label for="country">25. Are there any 3rd Party Sites involved  in the process</label>  
        
           <select  id="tps" onchange="funtional_point()" class="form-control" name="Third_party_sites" required>
                <option disabled selected value> -- select an option -- </option>
                <option value="Yes">Yes</option>
                     <option value="No">No</option>
                     
          </select>
        </div>
      <div class="form-group col-md-6">
          <label for="country">26. Number of Screens involved in the process
</label>  
        
        
          <input type="text-center" oninput="funtional_point()" id="nos" class="form-control"  maxlength="30" name="Num_of_scrs" onkeypress="return isNumberKey(event)" required>
        </div>
      </div>
    <div class="form-row">
      <div class="form-group col-md-6">
        <label for="subject">27. Number of  process steps
</label>
      
      
        <input id="subject" oninput="funtional_point()" class="form-control"  name="Num_of_proccessteps" onkeypress="return isNumberKey(event)" required>
      </div>
    <div class="form-group col-md-6">
          <label for="country">28. Number of Exceptions/alternate scenarios
</label>  
        
        
            <input type="number-center" oninput="funtional_point()"  id="nosc" class="form-control"  onkeypress="return isNumberKey(event)"    name="Num_of_Scenarios" required/>
        </div>
      </div>
      <div class="form-row">
        <div class="form-group col-md-6">
          <label for="country">29. Number of Decision points involved in the process
</label>  
        
            <input type="number-center" oninput="funtional_point()" id="nod" class="form-control"   onkeypress="return isNumberKey(event)" name="Num_of_Decpoints" required/>
        </div>
      <div class="form-group col-md-6">
          <label for="country">30. Number of Standard Input Template
</label>  
        
            <input type="number-center" oninput="funtional_point()" id="nost" class="form-control" name="Num_of_standardinput" onkeypress="return isNumberKey(event)" required>
        </div>
      </div>
      <div class="form-row">
        <div class="form-group col-md-6">
          <label for="sla">31. Interaction with Dynamic Table
</label>
        
          <select  class="form-control" onchange="funtional_point()" id="indt"  name="Intr_dynamic_table" required>
                <option disabled selected value> -- select an option -- </option>
                <option value="Yes">Yes</option>
                     <option value="No">No</option>
                     
          </select>
        </div>
      <div class="form-group col-md-6">
          <label for="fname">32. Number of Dynamic\Ajax based controls
</label>
        
        
            <input type="number-center" oninput="funtional_point()" id="nob" class="form-control"  step="any" maxlength="30" name="Num_of_basedcontrols" onkeypress="return isNumberKey(event)" required>
        </div>
      </div>
      
    
    

    
      <div class="form-row">
          <div class="form-group col-md-6">
            <label for="lname">33. Number of Access Profile to cover
</label>
          
            <input  class="form-control" oninput="funtional_point()" type="text-center" id="noap"  name="Num_of_accessprofile" onkeypress="return isNumberKey(event)" required>
          </div>
        <div class="form-group col-md-6">
              <label for="country">34. Number of Browser Support
</label>  
            
            <input  class="form-control" oninput="funtional_point()" type="text-center" id="nbs" name="Num_of_browsersupp" onkeypress="return isNumberKey(event)" required>
          </div>
          </div>
          <div class="form-row">
              <div class="form-group col-md-6">
                <label for="country">35. Operational stability of the applications and how often is it changed?</label>  
              
                      <select class="form-control" onchange="funtional_point()" id="opst"  name="Operation_stability" required>
                              <option disabled selected value> -- select an option -- </option>
                              <option value="Planned Downtime">Planned Downtime</option>
                              <option value="Once in a month">Once in a month</option>
                             </select>
              </div>
            <div class="form-group col-md-6">
              <label for="subject">36. Frequency of business process Change</label>
            
                  <select class="form-control"  onchange="funtional_point()" id="freqch" name="Freq_change" required>
                          <option disabled selected value> -- select an option -- </option>
                      <option value="Frequently">Frequently</option>
                      <option value="Infrequently">Infrequently</option>

                     </select>
            </div>
          </div>
          <div class="form-row">
             <div class="form-group col-md-6">
                <label for="country">37. Service Level Agreement of the process?</label>  
              
                      <select  class="form-control" onchange="funtional_point()" id="svc" name="Svc_lvl_agr" required>
                              <option disabled selected value> -- select an option -- </option>
                          <option value="Less than 8 hrs">Less than 8 hrs</option>
                          <option value="Less than or equal to 24 hrs">Less than or equal to 24 hrs</option>
                          <option value="More than 24 hrs">More than 24 hrs</option>
                         </select>
              </div>
            <div class="form-group col-md-6">
                <label for="country">38. Number of Business User to Get Sign-off
</label>  
              
                      <input  class="form-control" oninput="funtional_point()" type="text-center" id="nog"  name="Num_of_getsignoff" onkeypress="return isNumberKey(event)" required>
              </div>
            </div>
            <div class="form-row">
              <div class="form-group col-md-6">
                <label for="country">39. Number of Business Team Required for Environment Set-up
</label>  
              
                      <input  class="form-control" oninput="funtional_point()" type="text-center" id="noes"  name="Num_of_Envsetup" onkeypress="return isNumberKey(event)" required>
              </div>
            <div class="form-group col-md-6">
                <label for="sla">40. Funtional Point ( Ease of Implementation) X- Axis</label>
              
              
                      <input class="form-control"  type="number-center" id="funp" name="Func_point"  readonly>
              </div>
            </div>
            <div class="form-row">
              <div class="form-group col-md-6">
                <label for="sla">41. Monthly effort Savings (Y AXIS)
</label>
              
                      <input class="form-control"  type="number-center" id="meffs" name="Monthly_effsaving" readonly>
              </div>
            <div class="form-group col-md-6">
                <label for="sla">42. Effort (#pm)
</label>
              
              
                      <input class="form-control"  type="number-center" id="effo" name="Effort" readonly>
              </div>
            </div>

             <div class="form-row">
              <div class="form-group col-md-6">
                <label for="sla">43. Quadrant

</label>
              
              
                      <input class="form-control"  type="number-center" id="qua" name="Quadrant" readonly>
              </div>
            </div>
            
            
          
          
          <div id="last" align="center" class="form-group row">
    <div align="center" class="col-sm-10">
      <button align="center" type="submit" class="btn btn-success">Submit</button>
    </div>
  </div>
          
          
             
          
        
        </div>
          
  </form>
