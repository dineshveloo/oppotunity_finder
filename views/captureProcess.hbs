              <label for="country">13. Documentations of the process</label>  
            
            
                  <select  class="form-control" id="docpre" onchange="automation()" name="Doc_Present" required>
                          <option disabled selected value> -- select an option -- </option>
                           <option value="No Documenation">No Documenation</option>
                           <option value="Partial Documentation">Partial Documentation</option>
                           <option value="Detailed Documentation">Detailed Documentation</option>
                          </select>
            </div>
          
          <div class="form-group col-md-6">
                <label for="country">14. Rule based process</label>  
              
              
                      <select class="form-control"  id="rbased" onchange="ap()" name="Rule_Based" required>
                              <option disabled selected value> -- select an option -- </option>
                              <option value="High">High</option>
                              <option value="Medium">Medium</option>
                              <option value="Low">Low</option>
                             </select>
              </div>
            </div>
          <div class="form-row">
            <div class="form-group col-md-6">
              <label>15. Structure of Input Data</label>
            
            
