                      <input  class="form-control" oninput="funtional_point()" type="text-center" id="nog"  name="Num_of_getsignoff" onkeypress="return isNumberKey(event)" required>
           </div>
            
            
              <div class="form-group col-md-6">
                <label for="country">39. Number of Business Team Required for Environment Set-up
</label>  
              
                      <input  class="form-control" oninput="funtional_point()" type="text-center" id="noes"  name="Num_of_Envsetup" onkeypress="return isNumberKey(event)" required>
          </div>
