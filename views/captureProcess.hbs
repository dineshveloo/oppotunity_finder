    <div class="form-row">
        <div class="form-group col-md-6">
      
        <label for="fname">1. Client Name</label>
      
      
        <input type="text-center"  class="form-control" id="cname" maxlength="30" name="clientName" onkeypress="return isAlphabet(event)" required>
      </div>
    
    <div class="form-group col-md-6">
    
        <label for="lname">2. Business Unit</label>
      
      
        <input type="text-center" class="form-control" id="busiunit" maxlength="30" name="Buss_Unit" onkeypress="return isAlphabet(event)" required>
      
    </div>
    </div>
    <div class="form-row">
        <div class="form-group col-md-6">
        <label for="country">3. Sub Business Unit</label>  
      
      
        <input type="text-center" class="form-control" id="subbunit" maxlength="30" name="Sub_Buss_Unit" onkeypress="return isAlphabet(event)" required>
      </div>
    
   <div class="form-group col-md-6">

          <label for="country">4. Process Name</label>  
        
        
          <input type="text-center" class="form-control" id="pname" maxlength="30" name="Proc_Name" onkeypress="return isAlphabet(event)" required>
        
      </div>
      </div>
      <div class="form-row">
        <div class="form-group col-md-6">
          <label for="country">5. Process Id</label>  
        
        
          <input type="text-center"  class="form-control" id="procid" maxlength="30" name="Proc_Id" required>
        </div>
      <div class="form-group col-md-6">
    
        <label>6. Process Description</label>
      
      
        <input id="subj" class="form-control"  name="Proc_Desc" style="height:120px" required>
      </div>
    </div>
    <div class="form-row">
       <div class="form-group col-md-6">
          <label for="country">7. Monthly Volume</label>  
        
        
            <input type="number-center" class="form-control" oninput="calculate()" onkeypress="return isNumberKey(event)"  id="mv" maxlength="30" name="Mon_Vol" required/>
        </div>
      
      <div class="form-group col-md-6">
          <label for="country">8. AHT(in minutes)</label>  
        
    
            <input type="number-center" class="form-control" oninput="calculate()"  onkeypress="return isNumberKey(event)" id="aht" step="any" maxlength="30" name="AHT" required/>
        </div>
      </div>
      <div class="form-row">
        <div class="form-group col-md-6">
          <label for="country">9. FTE</label>  
        
        
            <input type="number-center"   class="form-control"  id="fte" step="any" maxlength="30" name="FTE" readonly>
        </div>
      
      <div class="form-group col-md-6">
        
          <label for="sla">10. SLA</label>
        
        
          <select  class="form-control" id="slacalc" name="SLA" required>
                <option disabled selected value> -- select an option -- </option>
                <option value="Less than 8 hrs">Less than 8 hrs</option>
                     <option value="Less than 12 hrs">Less than 12 hrs</option>
                     <option value="Less than 24 hrs">Less than 24 hrs</option>
                     <option value="Less than 48 hrs">Less than 48 hrs</option>
          </select>
        </div>
      </div>
      
    
    
    
    <div class="form-row">
        <div class="form-group col-md-6">
