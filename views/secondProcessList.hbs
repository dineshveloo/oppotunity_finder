  <form action="/user/updateDetailsTwo" method="POST">
    <div class="form-row">
      <div class="form-group col-md-6">
        <label for="fname">21. Process ID</label>
      
        <input type="text-center"  class="form-control" name="Proc_Id" value="{{secprocess.Proc_Id}}" readonly>
      </div>
   
      
        <input type="hidden"  class="form-control" name="clientName" value="{{list.clientName}}" readonly>
     


    
        <input type="hidden"  class="form-control" name="Buss_Unit" value="{{list.Buss_Unit}}" readonly>
      


    
        <input type="hidden"  class="form-control" name="Sub_Buss_Unit" value="{{list.Sub_Buss_Unit}}" readonly>
      

    
        <input type="hidden"  class="form-control" name="Proc_Name" value="{{list.Proc_Name}}" readonly>
     
    <div class="form-group col-md-6">
        <label for="fname">22. Number of applications involved in the process</label>
              <input type="text-center"  class="form-control" id="na" name="Num_of_apps" onkeypress="return isNumberKey(event)" value="{{secprocess.Num_of_apps}}"  readonly>
      </div>
    </div>
    <div class="form-row">
      <div class="form-group col-md-6">
        <label for="lname">23. Number of mainframe applications involved</label>
      
        <input type="text-center" id="nm" class="form-control" name="Num_of_mainframe" onkeypress="return isNumberKey(event)"  value="{{secprocess.Num_of_mainframe}}"  readonly>
      </div>
    <div class="form-group col-md-6">
        <label for="country">24. Number of applications to be accessed through Citrix</label>  
      
        <input type="text-center" class="form-control" id="nc" name="Num_of_Citrix" onkeypress="return isNumberKey(event)"  value="{{secprocess.Num_of_Citrix}}"  readonly>
      </div>
    </div>
    <div class="form-row">
        <div class="form-group col-md-6">
          <label for="country">25. Are there any 3rd Party Sites involved  in the process</label>  
        
           <select id="tps" class="form-control" name="Third_party_sites">
                
                <option>{{secprocess.Third_party_sites}}</option>
                <option id="21" value="Yes" disabled>Yes</option>
                     <option id="22" value="No" disabled>No</option>
                     
                
                     
          </select>
        </div>
      <div class="form-group col-md-6">
          <label for="country">26. Number of Screens involved in the process
</label>  
        
          <input type="text-center" id="ns" class="form-control"  maxlength="30" name="Num_of_scrs" onkeypress="return isNumberKey(event)" value="{{secprocess.Num_of_scrs}}"  readonly>
        </div>
      </div>
    <div class="form-row">
      <div class="form-group col-md-6">
        <label for="subject">27. Number of  process steps
</label>
      
        <input id="subject" class="form-control" id="np" name="Num_of_proccessteps" onkeypress="return isNumberKey(event)" value="{{secprocess.Num_of_proccessteps}}"  readonly>
      </div>
    <div class="form-group col-md-6">
          <label for="country">28. Number of Exceptions/alternate scenarios
</label>  
        
            <input type="number-center" class="form-control" id="ne" onkeypress="return isNumberKey(event)"    name="Num_of_Scenarios" value="{{secprocess.Num_of_Scenarios}}"  readonly/>
        </div>
      </div>
      <div class="form-row">
        <div class="form-group col-md-6">
          <label for="country">29. Number of Decision points involved in the process
</label>  
        
            <input type="number-center" class="form-control" id="nd"  onkeypress="return isNumberKey(event)" name="Num_of_Decpoints" value="{{secprocess.Num_of_Decpoints}}"  readonly/>
        </div>
      <div class="form-group col-md-6">
          <label for="country">30. Number of Standard Input Template
</label>  
        
            <input type="number-center" id="sit" class="form-control" name="Num_of_standardinput" onkeypress="return isNumberKey(event)" value="{{secprocess.Num_of_standardinput}}"  readonly>
        </div>
        </div>
        <div class="form-row">
     <div class="form-group col-md-6">
          <label for="sla">31. Interaction with Dynamic Table
</label>
        
          <select id="dt" class="form-control"  name="Intr_dynamic_table">
                
                <option>{{secprocess.Intr_dynamic_table}} </option>
                <option value="Yes"  id="23" disabled>Yes</option>
                     <option value="No" id="24" disabled>No</option>
                
          </select>
        </div>
      
      
        <div class="form-group col-md-6">
          <label for="fname">32. Number of Dynamic\Ajax based controls
</label>
        
            <input type="number-center"  id="nda" class="form-control"  step="any" maxlength="30" name="Num_of_basedcontrols" onkeypress="return isNumberKey(event)" value="{{secprocess.Num_of_basedcontrols}}"  readonly>
        </div>
        </div>
      
      <div class="form-row">
    <div class="form-group col-md-6">
            <label for="lname">33. Number of Access Profile to cover
</label>
          
            <input  class="form-control" id="nap" type="text-center"   name="Num_of_accessprofile" onkeypress="return isNumberKey(event)" value="{{secprocess.Num_of_accessprofile}}"  readonly>
          </div>
        
            <div class="form-group col-md-6">
              <label for="country">34. Number of Browser Support
</label>  
           
            <input  class="form-control" id="nbs" type="text-center"  name="Num_of_browsersupp" onkeypress="return isNumberKey(event)" value="{{secprocess.Num_of_browsersupp}}"  readonly>
          </div>
          </div>
          <div class="form-row">
               <div class="form-group col-md-6">
                <label for="country">35. Operational stability of the applications and how often is it changed?</label>  
              
                      <select class="form-control" id="os"  name="Operation_stability">
                              
                              <option>{{secprocess.Operation_stability}}</option>
                              <option id="25" value="Planned Downtime" disabled>Planned Downtime</option>
                              <option id="26" value="Once in a month" disabled>Once in a month</option>
                              
                             </select>
              </div>
            
            <div class="form-group col-md-6">
              <label for="subject">36. Frequency of business process Change</label>
            
                  <select id="fbc" class="form-control" name="Freq_change">
                          
                          <option>{{secprocess.Freq_change}}</option>
                      <option id="27" value="Frequently" disabled>Frequently</option>
                      <option id="28" value="Infrequently" disabled>Infrequently</option>

                      
                     </select>
            </div>
            </div>
            <div class="form-row">
          <div class="form-group col-md-6">
                <label for="country">37. Service Level Agreement of the process?</label>  
              
                      <select  class="form-control" id="sa" name="Svc_lvl_agr">
                              
                              <option>{{secprocess.Svc_lvl_agr}}</option>
                          <option id="29" value="Less than 8 hrs" disabled>Less than 8 hrs</option>
                          <option id="30" value="Less than or equal to 24 hrs" disabled>Less than or equal to 24 hrs</option>
                          <option id="31" value="More than 24 hrs" disabled>More than 24 hrs</option>

                          
                         </select>
              </div>
            
              <div class="form-group col-md-6">
                <label for="country">38. Number of Business User to Get Sign-off
</label>  
              
                      <input  class="form-control" id="nob" type="text-center"  name="Num_of_getsignoff" onkeypress="return isNumberKey(event)" value="{{secprocess.Num_of_getsignoff}}"  readonly>
              </div>
            </div>
            <div class="form-row">
            <div class="form-group col-md-6">
                <label for="country">39. Number of Business Team Required for Environment Set-up
</label>  
              
                      <input  class="form-control" type="text-center" id="nbt" name="Num_of_Envsetup" onkeypress="return isNumberKey(event)" value="{{secprocess.Num_of_Envsetup}}"  readonly>
              </div>
            
              <div class="form-group col-md-6">
                <label for="sla">40. Funtional Point ( Ease of Implementation) X- Axis</label>
              
                      <input class="form-control" id="fp" type="number-center" name="Func_point"  value="{{secprocess.Func_point}}"  readonly>
              </div>
              </div>
              <div class="form-row">
            <div class="form-group col-md-6">
                <label for="sla">41. Monthly effort Savings (Y AXIS)
</label>
              
                      <input class="form-control" id="mes" type="number-center" name="Monthly_effsaving" value="{{secprocess.Monthly_effsaving}}"  readonly>
              </div>
            
              <div class="form-group col-md-6">
                <label for="sla">42. Effort (#pm)
</label>
              
                      <input class="form-control" id="ef" type="number-center"  name="Effort" value="{{secprocess.Effort}}"  readonly>
              </div>
              </div>
            <div class="form-group col-md-6">
                <label for="sla">43. Quadrant

</label>
              
                      <input class="form-control" id="Qu" type="number-center"  name="Quadrant" value="{{secprocess.Quadrant}}"  readonly>
              </div>
           
            <div id="last" align="center" class="form-group row">
    <div align="center" class="col-sm-10">
     <input id="btnEdit" class="btn btn-success" type="button" value="Edit">
            <input type="submit" class="btn btn-success" value="Submit" id="su"> 
        
    </div>
  </div>
          
            
          
         
